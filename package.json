  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.19",
    "babel-core": "^5.8.19",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.22.0",
    "gh-pages-deploy": "^0.3.0",
    "jsdom": "^7.0.2",
    "react-addons-test-utils": "^0.14.2",
    "react-tools": "^0.13.3",
    "standard": "^5.3.1",
    "style-loader": "^0.13.0",
    "tap-spec": "^4.1.0",
    "tape": "^4.0.1",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
